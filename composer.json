{
    "name": "serendipity_hq/component-geo-builder",
    "description": "Parses the exports of countries from Geonames and exports the data in machine readable formats.",
    "type": "library",
    "license": "MIT",
    "keywords": ["geonames"],
    "homepage": "https://github.com/Aerendir/component-geo-names",
    "support": {
        "issues": "https://github.com/Aerendir/component-geo-names/issues"
    },
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "aerendir@serendipityhq.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "serendipity_hq/console-styles-bundle": "dev-dev",
        "symfony/dom-crawler": "^4.4|^5.1",
        "symfony/serializer": "^4.4|^5.1",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "ext-ast": "*",
        "bamarni/composer-bin-plugin": "^1.4",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.4|^5.1",
        "symfony/form": "^4.4|^5.1",
        "symfony/var-dumper": "^4.4|^5.1"
    },
    "suggest": {
        "symfony/form": "To use the builtin form types."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Component\\GeoBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Component\\GeoBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "bin": "echo 'bin not installed'",
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"]
    }
}
