{
    "name": "serendipity_hq/component-geo-builder",
    "description": "Parses the exports of countries from Geonames and exports the data in machine readable formats.",
    "type": "library",
    "license": "MIT",
    "keywords": ["geonames"],
    "homepage": "https://github.com/Aerendir/component-geo-names",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.5",
        "serendipity_hq/console-styles-bundle": "dev-dev",
        "symfony/dom-crawler": "^4.4|^5.0",
        "symfony/serializer": "^4.4|^5.0",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "drenso/phan-extensions": "@stable",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.4.6",
        "phpstan/phpstan": "^0.12.32",
        "phpstan/phpstan-phpunit": "@stable",
        "psalm/plugin-phpunit": "@stable",
        "rector/rector": "^0.6",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.4|^5",
        "symfony/form": "^4.4|^5",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/var-dumper": "^4.4|^5",
        "thecodingmachine/phpstan-safe-rule": "@stable",
        "vimeo/psalm": "3.8.3"
    },
    "suggest": {
        "symfony/form": "To use the builtin form types."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Component\\GeoBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Component\\GeoBuilder\\Tests\\": "tests"
        }
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
